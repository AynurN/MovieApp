
@model List<GenreGetVM>
        <!-- End of Sidebar -->
        <!-- Content Wrapper -->
      
                <!-- End of Topbar -->
                <!-- Begin Page Content -->
                <div class="container-fluid">

                    <!-- Page Heading -->
                    <h1 class="h3 mb-2 text-gray-800">Tables</h1>
                    
                    <!-- DataTales Example -->
                    <div class="card shadow mb-4">
                        <div class="card-header py-3">
                            <h6 class="m-0 font-weight-bold text-primary">DataTables Example</h6>
                        </div>
                        <div class="card-body">
                            <div class="table-responsive">
                                <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                                    <thead>
                                        <tr>
                                            <th>Id</th>
                                            <th>Name</th>
                                            <th>CreatedAt</th>
                                            <th>ModifiedAt</th>
                                            <th>IsDeleted</th>
                                            
                                        </tr>
                                    </thead>
                                    <tbody>
                        @foreach(GenreGetVM genre in Model)
                        {
                            <tr>
                                <td>@genre.Id</td>
                                <td>@genre.Name</td>
                                <td>@genre.CreatedAt</td>
                                <td>@genre.ModifiedAt</td>
                                <td>@genre.IsDeleted</td>

                            </tr>
                        }
                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>

                </div>
                <!-- /.container-fluid -->

            </div>
            <!-- End of Main Content -->
            <!-- Footer -->
          
